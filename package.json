    "start": "npm run start --workspace=./example"
  },
  "workspaces": [
    "example",
    "packages/*"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "^3.7.1",
    "typescript": "^4.6.3",
    "wrangler": "^2.0.3"
  }
}
